                  </Transition.Action>
                  <Transition.Condition>[guessNumber &lt; randomNumber]</Transition.Condition>
                </Transition>
                <Transition DisplayName="Matched" sap2010:WorkflowViewState.IdRef="Transition_4">
                  <Transition.To>
                    <State x:Name="__ReferenceID1" DisplayName="GameOver" sap2010:WorkflowViewState.IdRef="State_3" IsFinal="True" />
