                  </Transition.Action>
                  <Transition.Condition>[guessNumber = randomNumber]</Transition.Condition>
                </Transition>
                <Transition DisplayName="GoLower" sap2010:WorkflowViewState.IdRef="Transition_2" To="{x:Reference __ReferenceID0}">
                  <Transition.Action>
                    <ui:MessageBox Caption="{x:Null}" ChosenButton="{x:Null}" Buttons="Ok" DisplayName="Message Box" sap2010:WorkflowViewState.IdRef="MessageBox_2" Text="[&quot;Please go lower&quot;]" TopMost="True" />
                  </Transition.Action>
                  <Transition.Condition>[guessNumber &gt; randomNumber]</Transition.Condition>
